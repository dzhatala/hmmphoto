$bg_board=<bg_board_0><bg_board_1><bg_board_2>
   <bg_board_3><bg_board_4><bg_board_5> MARKER;

$fp_logo=<fp_logo_0><fp_logo_1><fp_logo_2>
   <fp_logo_3><fp_logo_4><fp_logo_5> MARKER;

$fp_screen=<fp_screen_0><fp_screen_1><fp_screen_2>
   <fp_screen_3><fp_screen_4><fp_screen_5> MARKER;

$fp_key=<fp_key_0><fp_key_1><fp_key_2>
   <fp_key_3><fp_key_4><fp_logo_5> MARKER;

/*** the phonetics sequence in the first*/
/**the row based sequence in the second grammar of fp_image
*/
$fp_image=
	{$bg_board } /**truncating upper and left**/
	<$fp_logo >
	<$fp_screen >
	<$fp_key >
	{$bg_board }

	|
	
	{bg_board }
	<fp_logo >
	<fp_screen >
	<fp_key >
	{bg_board };


$bg_red=<bg_red_0>  | 
		<bg_red_0_0><bg_red_0_1><bg_red_0_2><bg_red_0_3><bg_red_0_4><bg_red_0_5> ;
$bg_white=<bg_white_0>  | 
		<bg_white_0_0><bg_white_0_1><bg_white_0_2><bg_white_0_3><bg_white_0_4><bg_white_0_5>;
$bg_dark=<bg_dark_0>  | 
		<bg_dark_0_1><bg_dark_0_2><bg_dark_0_3><bg_dark_0_4><bg_dark_0_5>;

$st_paper=
	<$bg_red MARKER>
	<$bg_white MARKER>
	<$bg_dark MARKER>|
	
	/**latest */
	{$bg_dark MARKER}
	<$bg_white MARKER>
	{$bg_dark MARKER}
	
	;

( $st_paper | $fp_image)


/* impossible in dict because the row number is not constant 
( st_paper | fp_image)

*/
